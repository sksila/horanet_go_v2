<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="horanet_assets_frontend" inherit_id="website.assets_frontend" name="Horanet foyer manager">
        <xpath expr="//script[last()]" position="after">
            <script  src="/horanet_auth_signup/static/src/js/auth_signup.js" type="text/javascript"></script>
        </xpath>

        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/horanet_auth_signup/static/src/css/auth_signup.css"
                  type="text/css"/>
        </xpath>
    </template>
</odoo>
