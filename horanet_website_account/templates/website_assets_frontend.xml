<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="horanet_assets_frontend" inherit_id="website.assets_frontend" name="Horanet foyer manager">
        <xpath expr="//script[last()]" position="after">
            <script src="/horanet_website_account/static/src/js/page_cru_member.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/page_my_foyers.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/page_my_account.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/partner_address.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/partner_invoice_address.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/partner_shipping_address.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/partner_image.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/partner_birthdate.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/page_my_company.js"
                    type="text/javascript"></script>
            <script src="/horanet_website_account/static/src/js/partner_phone.js"
                    type="text/javascript"></script>
        </xpath>

        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/horanet_website_account/static/src/css/website_account.css"
                  type="text/css"/>
        </xpath>
    </template>

</odoo>
