<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_form_collectivity_config_settings" model="ir.ui.view">
        <field name="name">TCO Transport Settings</field>
        <field name="model">horanet.transport.config</field>
        <field name="inherit_id" ref="horanet_transport.view_form_horanet_transport_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='group_settings_contact']" position="after">
                <group name="tco_transport_terminal_settings" string="TCO Transport Terminals Settings">
                    <field name="terminal_lb7_directory_path"/>
                    <field name="lb7_time_offset"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
