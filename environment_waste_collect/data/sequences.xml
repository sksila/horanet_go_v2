<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_pickup_contract" model="ir.sequence">
            <field name="name">Pickup Contract</field>
            <field name="code">seq_environment_pickup_contract</field>
            <field name="prefix">CN%(day)s/%(month)s/%(year)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_pickup_request" model="ir.sequence">
            <field name="name">Pickup Request</field>
            <field name="code">seq_environment_pickup_request</field>
            <field name="prefix">PR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
