<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="assets_backend" name="Horanet backend sources" inherit_id="web.assets_backend">

            <!-- Backend javascript sources -->
            <xpath expr="//script[last()]" position="after">
                <script src="/horanet_web/static/src/js/backend/sheet_wider.js"
                        type="text/javascript"></script>
                <script src="/horanet_web/static/src/js/backend/widget_list_button_redirect.js"
                        type="text/javascript"></script>
                <!--Field binary modified for large files-->
                <script src="/horanet_web/static/src/js/backend/widget_form_binary_large.js"
                        type="text/javascript"></script>
                <!--Better gauge widget-->
                <script src="/horanet_web/static/src/js/backend/widget_kanban_better_gauge.js"
                        type="text/javascript"></script>
                <!--Modification de la vue diagram-->
                <script src="/horanet_web/static/src/js/backend/inherit_view_diagram.js"
                        type="text/javascript"></script>
            </xpath>

            <!-- Backend CSS sources -->
            <xpath expr="//link[last()]" position="after">
                <link href='/horanet_web/static/src/css/backend/sheet_wider.css'
                      type="text/css" rel='stylesheet'/>
                <link href='/horanet_web/static/src/css/backend/widget_list_button_redirect.css'
                      type="text/css" rel='stylesheet'/>
            </xpath>
        </template>
    </data>
</openerp>
