<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- URL of SmartEco backend -->
        <record model="ir.config_parameter" id="backend_url_tpa_smarteco">
            <field name="key">tpa.smarteco.backend_url</field>
            <field name="value">/default/url/</field>
        </record>
        <!-- Activation of SmartEco synchronization -->
        <record model="ir.config_parameter" id="tpa_smarteco_enable">
            <field name="key">tpa.smarteco.is_enable</field>
            <field name="value">False</field>
        </record>
        <!-- Method of SmartEco backend -->
        <record model="ir.config_parameter" id="method_tpa_smarteco_partner">
            <field name="key">tpa.smarteco.partner_method</field>
            <field name="value">backendOdooPartner</field>
        </record>
    </data>
</odoo>
