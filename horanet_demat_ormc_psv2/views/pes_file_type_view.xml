<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_pes_file_type_form" model="ir.ui.view">
            <field name="name">PES file type form</field>
            <field name="model">pes.file.type</field>
            <field name="arch" type="xml">
                <form string="File Type">
                    <header>
                    </header>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_pes_file_type_tree" model="ir.ui.view">
            <field name="name">PES file type tree</field>
            <field name="model">pes.file.type</field>
            <field name="arch" type="xml">
                <tree string="File Type">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="pes_file_type_action">
            <field name="name">PES File Type</field>
            <field name="res_model">pes.file.type</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>
