<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" name="Frontend sources" inherit_id="website.assets_frontend">

        <!-- Frontend javascript sources -->
        <xpath expr="//script[last()]" position="after">
            <script src="/fullcalendar/static/src/lib/fullcalendar/2.9.1/fullcalendar.min.js"
                    type="text/javascript"></script>
            <script type="text/javascript" src="/fullcalendar/static/src/lib/fullcalendar/2.9.1/lang-all.js"></script>
            <script type="text/javascript" src="/fullcalendar/static/src/js/fullcalendar-locale.js"></script>
        </xpath>

        <!-- Frontend CSS sources -->
        <xpath expr="//link[last()]" position="after">
            <link href='/fullcalendar/static/src/lib/fullcalendar/2.9.1/fullcalendar.min.css' type="text/css"
                  rel='stylesheet'/>
        </xpath>
    </template>
</odoo>
