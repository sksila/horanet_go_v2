<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_form_collectivity_config_settings" model="ir.ui.view">
        <field name="name">contact personal information setting</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="horanet_go.view_form_collectivity_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='group_settings_contact_detail']" position="inside">
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_left_pane"/>
                    <div class="o_setting_right_pane">
                        <label for="group_contact_personal_information"/>
                        <div class="mt16">
                            <field name="group_contact_personal_information" class="o_light_label" widget="radio"/>
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_left_pane">
                        <field name="group_add_personal_gender"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="group_add_personal_gender"/>
                    </div>
                </div>
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_left_pane">
                        <field name="group_add_personal_birth_date"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="group_add_personal_birth_date"/>
                    </div>
                </div>
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_left_pane">
                        <field name="group_add_personal_birth_place"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="group_add_personal_birth_place"/>
                    </div>
                </div>
                <div class="col-xs-12 col-md-6 o_setting_box">
                    <div class="o_setting_left_pane">
                        <field name="group_add_personal_family_quotient"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="group_add_personal_family_quotient"/>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>